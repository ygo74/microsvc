services:
  sampleservicemanagement.aspnetcore:
    build:
      context: D:\devel\Azure\git\microsvc\src\BuildingBlocks\ServiceManagement\src
      dockerfile: ../samples/SampleServiceManagement.AspNetCore/SampleServiceManagement.AspNetCore/Dockerfile
      target: base
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: sampleservicemanagementaspnetcore:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  bin/Debug/netcoreapp2.0/SampleServiceManagement.AspNetCore.dll'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/bash -c "if PID=$$(pidof
        -x dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    volumes:
    - D:\devel\Azure\git\microsvc\src\BuildingBlocks\ServiceManagement\samples\SampleServiceManagement.AspNetCore\SampleServiceManagement.AspNetCore:/app:rw
    - C:\Users\Administrator\vsdbg\vs2017u5:/remote_debugger:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\Administrator\.nuget\packages:/root/.nuget/packages:ro
version: '3.4'